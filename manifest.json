{
    "developer": {
        "name": "Martin Giger",
        "url": "https://humanoids.be"
    },
    "homepage_url": "https://github.com/freaktechnik/coupon-booklet",
    "manifest_version": 3,
    "version": "1.2.3",
    "name": "__MSG_extensionName__",
    "icons": {
        "96": "coupon-booklet.svg"
    },
    "action": {
        "default_icon": "coupon-booklet.svg",
        "default_title": "__MSG_extensionName__",
        "default_popup": "booklet.html"
    },
    "commands": {
        "__execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+B"
            },
            "description": "__MSG_shortcut__"
        }
    },
    "permissions": [
        "clipboardWrite",
        "unlimitedStorage",
        "tabs",
        "alarms"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "coupon-booklet@humanoids.be",
            "data_collection_permissions": {
                "required": ["none"]
            },
            "strict_min_version": "134.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "default_locale": "en"
}
